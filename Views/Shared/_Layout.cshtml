<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width" />
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
   
   
    @Styles.Render("~/Content/mobileCss", "~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @*Рефернца за користење на google maps библиотеките*@

    <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
    @*Рефернцирање на jquery 1.7.1 библиотеката*@
    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery-1.7.1/jquery.min.js")"></script>
    @*Рефернцирање на jquery ui библиотеката за мапи*@
    <script type="text/javascript" src="@Url.Content("~/ui/jquery.ui.map.js")"></script>
    @*Рефернцирање на jquery ui библиотеката*@
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.min.js")" type="text/javascript"></script>
    @*Рефернцирање на jquery mobile 1.1.0 библиотеката*@
    <script src="@Url.Content("~/Scripts/jquery.mobile-1.1.0.js")" type="text/javascript"></script>
    @*Рефернцирање на main.js javascript фајлот*@
    <script src="@Url.Content("~/Scripts/main.js")" type="text/javascript"></script>
    @*Рефернцирање на jquery.cyrillic.js библиотеката за конверзија на латичните во кирилични букви *@
    <script src="@Url.Content("~/Scripts/jquery.cyrillic.js")" type="text/javascript"></script>
    @*Рефернцирање на jquery.bpopup-0.7.0.min.js библиотеката за popup-от со листата на градови*@
    <script src="@Url.Content("~/Scripts/jquery.bpopup-0.7.0.min.js")" type="text/javascript"></script>

    
    

    <link rel="stylesheet" href="@Url.Content("~/CSS/jqx.base.css")" type="text/css" />
    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery-1.8.3.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/jqxcore.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/jqxdata.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/jqxbuttons.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/jqxscrollbar.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/jqxlistbox.js")"></script>


    

    @*<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false"></script>*@

    @*Рефернцирање на main.css фајлот со стиловите за сите елементи*@
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />

    <script type="text/javascript" charset="UTF-8" src="http://maps.google.com/maps/api/js?sensor=false&v=3&libraries=geometry&language=mk"></script>
</head>
<body>
    <div data-role="page" data-theme="f" data-transition="slide">
        <div data-role="header" class="headerF" style="height: 50px;">
            <span class="spanBack" style="display: none">
                <div class="backBig">
                    <input type="button" class="btnBack" value="Назад" onclick="btnBackClick()" data-icon="back"
                        data-mini="true" />
                </div>
                <div class="backSmall">
                    <img src="~/Content/Images/back.png" class="imgBack btnBack" onclick="btnBackClick()" />
                </div>
            </span><span class="spanLogo">
                <img src="~/Content/Images/logo-fzom.png" />
            </span><span class="spanHome">
                <div class="homeSmall" style="right: 5px">
                    <img src="~/Content/Images/home.png" onclick="vratiPocetna()" alt="" />
                </div>
                <div class="homeBig" style="right: 0px;">
                    <input type="button" class="btnHome" value="Почетна" onclick="vratiPocetna()" data-icon="home"
                        data-iconpos="right" data-mini="true" />
                </div>
            </span>
            @if (IsSectionDefined("Header"))
            {
                @RenderSection("Header")
            }
        </div>
@*        <div data-role="header">
            <span class="logo_apteki">
                <img src="~/Content/Images/logo-apteki.png" />
            </span>
        </div>*@
        <div data-role="content" class="contentF" style="position: relative; padding-top: 30px">
            @RenderBody()
        </div>
        <div data-role="footer" class="footerF" data-position="fixed">
            <span>
                <input id="uptotop" type="button" class="btnNajgore" data-mini="true" data-icon="arrow-u"
                    data-theme="a" value="Најгоре" />
            </span>

            <span>
               
            </span>

            <span>
                <input id="basicSr" type="button" class="btnBasic" data-mini="true" data-icon="arrow-u"
                    data-theme="a" value="Напредно пребарување" onclick="window.location.href='AdvancedSearch'"/>
            </span>

            <span>
                <input id="defaultSr" type="button" class="btnSettings" data-mini="true" data-icon="arrow-u"
                    data-theme="a" value="Администратор" onclick="setPassword()"/>
            </span>
            <span>
                <input id="defaultSr" type="button" class="btnSettings" data-mini="true" data-icon="arrow-u"
                    data-theme="a" value="Почетна" onclick="window.location.href='Home'"/>
            </span>

        </div>
    </div>
    @RenderSection("scripts", required: false)
</body>
</html>
